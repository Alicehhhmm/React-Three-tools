{
    "name": "rose-pro",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "pnpm vite",
        "dev": "pnpm vite",
        "build": "tsc && vite build",
        "lint:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:prettier": "prettier --write --log-level warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
        "lint:stylelint": "stylelint \"**/*.{css,less,scss}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
        "prepare": "husky install",
        "preview": "vite preview",
        "lint:lint-staged": "lint-staged"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
            "prettier --write--parser json"
        ],
        "package.json": [
            "prettier --write"
        ],
        "*.json": [
            "prettier --write"
        ],
        "*.{scss,less,styl}": [
            "stylelint --fix",
            "prettier --write"
        ],
        "*.md": [
            "prettier --write"
        ]
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "dependencies": {
        "@types/node": "^20.9.1",
        "antd": "^5.11.1",
        "axios": "^1.6.2",
        "lodash-es": "^4.17.21",
        "qs": "^6.11.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.2",
        "@commitlint/config-conventional": "^18.4.2",
        "@types/lodash-es": "^4.17.11",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "commitizen": "^4.3.0",
        "dotenv": "^16.3.1",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "husky": "^8.0.3",
        "less": "^4.2.0",
        "lint-staged": "^15.1.0",
        "postcss-less": "^6.0.0",
        "prettier": "^3.1.0",
        "prettier-plugin-packagejson": "^2.4.6",
        "stylelint": "^15.11.0",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-recess-order": "^4.3.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-less": "^2.0.0",
        "typescript": "^5.2.2",
        "vite": "^4.3.9"
    },
    "packageManager": "pnpm@8.1.0",
    "engines": {
        "node": ">=16.15.1",
        "pnpm": ">=8.1.0"
    }
}
